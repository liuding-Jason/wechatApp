<!--pages/diamond/index.wxml-->
<import src="/dist/noticebar/index.wxml" />
<import src="/dist/tab/index.wxml" />
<swiper indicator-dots="{{indicatorDots}}" indicator-color="{{indicatorColor}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" style="height : 320rpx;">
  <block wx:for="{{imgUrls}}" wx:key="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
<view style='position:relative ;z-index : 1;left : 0 ;top : -146px;'>
  <view class="zan-panel" >
    <template
      is="zan-noticebar"
      data="{{ ...movable, componentId: 'movable'  }}"
    ></template>
  </view>
</view>
<view>
  <view class="zan-row">
    <view class="zan-col zan-col-4">
      span: 8
    </view>
    <view class="zan-col zan-col-4">
      span: 8
    </view>
    <view class="zan-col zan-col-4">
      span: 8
    </view>
    <view class="zan-col zan-col-4">
      span: 8
    </view>
